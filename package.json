{
  "name": "key-manager-ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=20.0.0 < 21.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:docker": "vite --host",
    "build": "tsc && vite build",
