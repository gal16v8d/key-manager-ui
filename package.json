    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.3",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jsdom": "^22.0.0",
    "msw": "^2.6.5",
    "node-fetch": "^3.3.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "msw": {
    "workerDirectory": "public"
  }
